        <tri_uid>68165130768fb1c51b8c548056455951</tri_uid>
        <tri_title>Ajax</tri_title>
        <tri_description></tri_description>
        <pro_uid>763275685688b4b1b3e7363045948797</pro_uid>
        <tri_type>SCRIPT</tri_type>
        <tri_webbot><![CDATA[$action = $_POST['action'] ?? '';

if ($action == "check_duplicate_user") {

  $national_id = $_POST['national_id'];
  $postal_code = $_POST['postal_code'];

  try {
    $sql_select = "SELECT id FROM users_custom
                   WHERE national_id = '$national_id' OR postal_code = '$postal_code'
                   LIMIT 1";
    $sql_result = executeQuery($sql_select);

    if (is_array($sql_result) && count($sql_result) > 0) {
      echo json_encode([
        'result' => true,
        'message' => 'کاربری با این کد ملی یا کد پستی قبلاً ثبت شده است.'
      ]);
    } else {
      echo json_encode([
        'result' => false,
        'message' => 'اطلاعات تکراری نیست و قابل ثبت است.'
      ]);
    }

  } catch (Exception $e) {
    echo json_encode([
      'result' => false,
      'message' => 'خطا در اجرای کوئری: ' . $e->getMessage()
    ]);
  }

  exit;
}
]]></tri_webbot>
