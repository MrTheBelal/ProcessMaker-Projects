        <tri_type>SCRIPT</tri_type>
        <tri_webbot><![CDATA[// ====== مرحله 1: گرفتن داده های فرم ======
$nationalId = @@txtNationalID_1;    // کد ملی
$postalCode = @@txtPostalCode;      // کد پستی
$password   = @@txtPassword;        // پسورد
$firstName  = "Mr.";
$lastName   = @@txtFullName_1;      // نام کامل
$email      = @@txtEmail;           // ایمیل
$role       = 'PROCESSMAKER_OPERATOR';
$phone		= @@txtMobile_1;
$mobile		= @@txtPhone_1;

// ====== مرحله 3: ساخت کاربر جدید ======
$userUID = PMFCreateUser($nationalId, $password, $firstName, $lastName, $email, $role);

$groupName = 'New';
$query = "SELECT GRP_UID FROM GROUPWF WHERE GRP_TITLE = '$groupName'";
$result = executeQuery($query);




$insertQuery = "INSERT INTO users_custom (id, full_name, national_id, phone, mobile, postal_code, created_at) 
VALUES ('$nationalId', '$lastName', '$nationalId', '$phone', '$mobile', '$postalCode', CURRENT_TIMESTAMP)";
    executeQuery($insertQuery);]]></tri_webbot>
