        <dyn_content><![CDATA[{"name":"SignUpForm","description":"","items":[{"type":"form","variable":"","var_uid":"","dataType":"","id":"495347653688b4c5f36c881013286226","name":"SignUpForm","description":"","mode":"edit","script":{"type":"js","code":"$('.pmdynaform-field-subtitle:not(#subDocuments)')\n  .css('background', '#96e0b4')\n  .css('color', '#01662a');\n\n$('#subDocuments')\n  .css('background', '#01662a')\n  .css('color', '#96e0b4');\n\n\n$(\"#radApplicantType\").setOnchange(function () {\n\n  function toggleFields(fields, enabled) {\n    fields.forEach(function (id) {\n      const $ctrl = $(\"#\" + id).getControl();\n      $ctrl.attr(\"disabled\", !enabled);\n\n      //  به جای prop(\"required\") از validation اختصاصی ProcessMaker استفاده شد\n      if (enabled) {\n        $(\"#\" + id).enableValidation();\n      } else {\n        $(\"#\" + id).disableValidation();\n      }\n    });\n  }\n\n  const realFields = [\n    \"txtFullName_1\",\n    \"txtNationalId_1\",\n    \"datBirthday\",\n    \"txtFatherName\",\n    \"txtBirthLocation\",\n    \"txtCertID\",\n    \"txtMobile\",\n    \"txtPhone\",\n    \"radGender\"\n  ];\n\n  const legalFields = [\n    \"txtFullName_2\",\n    \"txtNationalID_2\",\n    \"txtMobile_2\",\n    \"txtPhone_2\"\n  ];\n\n  if ($(\"#radApplicantType\").getValue() === \"legal\") {\n    toggleFields(realFields, false);\n    toggleFields(legalFields, true);\n  }\n\n  if ($(\"#radApplicantType\").getValue() === \"real\") {\n    toggleFields(legalFields, false);\n    toggleFields(realFields, true);\n  }\n});\n\n\n//  بررسی صحت کد ملی و کد پستی + کنترل تکراری بودن با AJAX\n$(\"form\").on(\"submit\", function () {\n  \n  let nationalId = $(\"#txtNationalId_1\").getValue().trim();\n  let postalCode = $(\"#txtPostalCode_1\").getValue().trim();\n  let valid = true;\n\n  $(\"#txtNationalId_1\").getControl().css(\"border\", \"\");\n  $(\"#txtPostalCode_1\").getControl().css(\"border\", \"\");\n\n  // بررسی الگوی کد ملی\n  if (!/^[1-9][0-9]{9}$/.test(nationalId)) {\n    $(\"#txtNationalId_1\").getControl().css(\"border\", \"2px solid red\");\n    valid = false;\n  }\n\n  // بررسی الگوی کد پستی\n  if (!/^\\d{10}$/.test(postalCode)) { \n    $(\"#txtPostalCode_1\").getControl().css(\"border\", \"2px solid red\");\n    valid = false;\n  }\n\n  if (!valid) {\n    alert(\"لطفاً کد ملی و کد پستی را به درستی وارد کنید.\");\n    return false;\n  }\n\n  // ====== بررسی تکراری بودن از طریق Ajax ======\n  $.ajax({\n    type: \"POST\",\n    url: window.location,\n    dataType: \"json\",\n    async: true,\n    data: {\n      action: \"check_duplicate_user\",\n      national_id: nationalId,\n      postal_code: postalCode\n    },\n    success: function (jsn) {\n      console.log(jsn);\n\n      if (jsn.result === true) {\n        alert(jsn.message || \"کاربری با این اطلاعات قبلاً ثبت شده است!\");\n      } else if (jsn.result === false) {\n        $(\"form\")[0].submit();\n      } else {\n        alert(\"پاسخ نامعتبر از سرور!\");\n      }\n    },\n    error: function (xhr, status, error) {\n      alert(\"خطا در ارتباط با سرور: \" + error);\n    }\n  });\n\n  return false; \n});\n"},"language":"en","externalLibs":"","printable":false,"items":[[{"type":"radio","variable":"radApplicantType","var_uid":"105412319688b4cd4a07670076191511","dataType":"string","protectedValue":false,"id":"radApplicantType","name":"radApplicantType","label":"نوع متقاضی","tabIndex":"","defaultValue":"real","hint":"","ariaLabel":"","ariaLabelVisible":true,"required":true,"requiredFieldErrorMessage":"لطفا این بخش را پر کنید","mode":"parent","datasource":"database","dbConnection":"workflow","dbConnectionLabel":"PM Database","sql":"","memoryCache":false,"dataVariable":"","options":[{"value":"real","label":"حقیقی"},{"value":"legal","label":"حقوقی"}],"var_name":"radApplicantType","colSpan":12}],[{"type":"subtitle","id":"subtitle0000000001","label":"مشخصات مالک حقیقی","ariaLabel":"","colSpan":12}],[{"type":"text","variable":"txtFullName_1","var_uid":"56403158