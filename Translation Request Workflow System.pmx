        <dyn_content><![CDATA[{"name":"\u0641\u0631\u0645 \u0628\u0631\u0631\u0633\u06cc \u062b\u0628\u062a \u067e\u06cc\u0634\u0646\u0647\u0627\u062f \u0648 \u0632\u0645\u0627\u0646 \u062a\u0648\u0633\u0637 \u0645\u0634\u062a\u0631\u06cc","description":"","items":[{"type":"form","variable":"","var_uid":"","dataType":"","id":"583118596689f764b2189e0065959219","name":"\u0641\u0631\u0645 \u0628\u0631\u0631\u0633\u06cc \u062b\u0628\u062a \u067e\u06cc\u0634\u0646\u0647\u0627\u062f \u0648 \u0632\u0645\u0627\u0646 \u062a\u0648\u0633\u0637 \u0645\u0634\u062a\u0631\u06cc","description":"","mode":"edit","script":{"type":"js","code":"\/\/ Approve or Reject handler \nfunction approval(action) {\n  if (confirm('\u0645\u0637\u0645\u0626\u0646\u06cc\u062f \u06a9\u0647 \u0645\u06cc\u062e\u0648\u0627\u0647\u06cc\u062f ' + action + ' \u06a9\u0646\u06cc\u062f \u061f')) {\n\n    let rowCount = $(\"#grdSuggests\").getNumberRows();\n    let selectedCount = 0;\n    let chosenRow = -1;\n\n    for (let i = 1; i <= rowCount; i++) {\n      let checked = $(\"#grdSuggests\").getValue(i, 5); \/\/ \u0633\u062a\u0648\u0646 chkSelect\n      if (checked == \"1\") {\n        selectedCount++;\n        chosenRow = i;\n      }\n    }\n\n    if (selectedCount === 0) {\n      alert(\"\u0644\u0637\u0641\u0627\u064b \u06cc\u06a9 \u0645\u062a\u0631\u062c\u0645 \u0631\u0627 \u0627\u0646\u062a\u062e\u0627\u0628 \u06a9\u0646\u06cc\u062f.\");\n      return false;\n    }\n    if (selectedCount > 1) {\n      alert(\"\u0641\u0642\u0637 \u0628\u0627\u06cc\u062f \u06cc\u06a9 \u0645\u062a\u0631\u062c\u0645 \u0627\u0646\u062a\u062e\u0627\u0628 \u0634\u0648\u062f.\");\n      return false;\n    }\n\n    let translator = $(\"#grdSuggests\").getText(chosenRow, 1);\n\n    $(\"#hidTranslatorName\").setValue(translator);\n\n\n    if (action === '\u062a\u0627\u06cc\u06cc\u062f') {\n      $(\"#hidApproved\").setValue(1);\n    }\n    if (action === '\u0631\u062f') {\n      $(\"#hidApproved\").setValue(0);\n    }\n\n    $(\"form\").submit();\n  } else {\n    return false;\n  }\n}\n\n\/\/ Approve form \n$(\"#btnAccept\").find(\"button\").on(\"click\", function() {\n  return approval('\u062a\u0627\u06cc\u06cc\u062f');\n});\n\n\/\/ Reject form \n$(\"#btnReject\").find(\"button\").on(\"click\", function() {\n  return approval('\u0631\u062f');\n});\n"},"language":"en","externalLibs":"","printable":false,"items":[[{"type":"title","id":"title0000000001","label":"\u0641\u0631\u0645 \u0645\u0628\u0627\u0644\u063a \u0648 \u0632\u0645\u0627\u0646 \u0645\u0648\u0631\u062f \u0646\u06cc\u0627\u0632 \u0648\u0627\u0631\u062f \u0634\u062f\u0647 \u062a\u0648\u0633\u0637 \u0645\u062a\u0631\u062c\u0645\u0627\u0646","ariaLabel":"","colSpan":12}],[{"type":"grid","variable":"grdSuggests","var_uid":"259083812689f76c3a2b381024743328","dataType":"grid","protectedValue":false,"id":"grdSuggests","name":"grdSuggests","label":"\u067e\u06cc\u0634\u0646\u0647\u0627\u062f\u0627\u062a \u0645\u062a\u0631\u062c\u0645\u0627\u0646","hint":"","required":false,"requiredFieldErrorMessage":"","columns":[{"type":"text","variable":"","var_uid":"","dataType":"","protectedValue":false,"id":"txtTranslator","name":"txtTranslator","label":"\u0646\u0627\u0645 \u0645\u062a\u0631\u062c\u0645","tabIndex":"","defaultValue":"","placeholder":"","hint":"","ariaLabel":"","required":false,"requiredFieldErrorMessage":"","textTransform":"none","validate":"","validateMessage":"","maxLength":1000,"formula":"","mode":"parent","operation":"","dbConnection":"workflow","dbConnectionLabel":"PM Database","sql":"","memoryCache":false,"columnWidth":"10","width":100,"title":"\u0646\u0627\u0645 \u0645\u062a\u0631\u062c\u0645","var_name":"grdSuggests"},{"type":"text","variable":"","var_uid":"","dataType":"","protectedValue":false,"id":"txtTime_1","name":"txtTime_1","label":"\u0632\u0645\u0627\u0646 \u0645\u0648\u0631\u062f \u0646\u06cc\u0627\u0632","tabIndex":"","defaultValue":"","placeholder":"","hint":"","ariaLabel":"","required":false,"requiredFieldErrorMessa