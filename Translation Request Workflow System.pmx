      <record>
        <tri_uid>57181005268a4c24e58c5b1083225169</tri_uid>
        <tri_title><![CDATA[ثبت ایدی مترجم از روی اسم]]></tri_title>
        <tri_description></tri_description>
        <pro_uid>550542860689dd49b51b2e7062616081</pro_uid>
        <tri_type>SCRIPT</tri_type>
        <tri_webbot><![CDATA[// مقدار username (مثلاً از فرم یا متغیر ورودی)
$username = @@hidTranslatorName;  

// کوئری
$query = "SELECT USR_UID FROM USERS WHERE USR_USERNAME = '$username'";

// اجرا
$result = executeQuery($query);

// بررسی نتیجه
if (is_array($result) && count($result) > 0) {
    @@hidTranslator = $result[1]['USR_UID'];
} else {
    @@hidTranslator = '';
}
]]></tri_webbot>
        <tri_param></tri_param>
      </record>
      <record>
        <tri_uid>78538842168a4bfd0f0eb32032616780</tri_uid>
        <tri_title><![CDATA[حذف اولین ردیف گرید]]></tri_title>
        <tri_description></tri_description>
        <pro_uid>550542860689dd49b51b2e7062616081</pro_uid>
        <tri_type>SCRIPT</tri_type>
        <tri_webbot><![CDATA[// گرفتن مقدار گرید
$grid = @@grdSuggests;

// چک کردن اینکه خالی نباشه
if (is_array($grid) && count($grid) > 0) {
    
    // بررسی اولین ردیف
    $firstRow = $grid[1];
    
    $allEmpty = true;
    foreach ($firstRow as $col) {
        if (trim($col) !== '') {
            $allEmpty = false;
            break;
        }
    }
}
]]></tri_webbot>
        <tri_param></tri_param>
      </record>
      <record>
        <tri_uid>83874956768a479faddc211080932019</tri_uid>
