        <tri_description></tri_description>
        <pro_uid>550542860689dd49b51b2e7062616081</pro_uid>
        <tri_type>SCRIPT</tri_type>
        <tri_webbot><![CDATA[@@hidTranslator = @@USER_LOGGED;

// مقدار UID (مثلاً از hidCustomer یا متغیر لاگین)
$uid = @@hidTranslator;

// کوئری
$query = "SELECT USR_USERNAME FROM USERS WHERE USR_UID = '$uid'";

// اجرا
$result = executeQuery($query);

// بررسی نتیجه
if (is_array($result) && count($result) > 0) {
    @@hidTranslatorName = $result[1]['USR_USERNAME'];
} else {
    @@hidTranslatorName = '';
}
]]></tri_webbot>
