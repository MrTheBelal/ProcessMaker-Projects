        <tri_title><![CDATA[ثبت پیشنهاد مترجمان]]></tri_title>
        <tri_description></tri_description>
        <pro_uid>550542860689dd49b51b2e7062616081</pro_uid>
        <tri_type>SCRIPT</tri_type>
        <tri_webbot><![CDATA[$newRow = [
    'translator'   => @@USER_LOGGED,
    'price'        => @@txtPrice,
    'time'         => @@txtTime,
    'description'  => @@txrDescription
];

if (is_array(@@grdSuggests)) {
    $gridData = @@grdSuggests;
} else {
    $gridData = [];
}
$gridData[] = $newRow;
@@grdSuggests = $gridData;]]></tri_webbot>
